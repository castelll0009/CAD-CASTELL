---
import Layout from "../layouts/layout.astro";

//components
import Hero from '../components/landing/Hero.astro';
import Features from '../components/landing/Features.astro';
import More from '../components/landing/More.astro';
import About from '../components/landing/About.astro';
import Projects from '../components/landing/Projects.astro'
import Services from "../components/landing/Services.astro";

//fetch form strapi
import fetchApi from '../lib/strapi';
import type Tour from '../interfaces/tour';

const tours = await fetchApi<Tour[]>({
  endpoint: 'tours', // the content type to fetch
  wrappedByKey: 'data', // the key to unwrap the response
});

console.log(tours);
---

<Layout title="Home">	

	<Hero></Hero>
	<Services></Services>	
	<More></More>
	<Features></Features>
	<About></About>
	<Projects></Projects>		
	
</Layout>